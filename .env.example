# Voice Interview AI - Environment Variables
# Copy this to .env and customize for your environment

# AWS Configuration (uses your AWS CLI credentials automatically)
AWS_REGION=us-east-1

# AWS Resources (will be created by Terraform)
S3_BUCKET=voice-interview-platform-storage-dev
SESSIONS_TABLE=voice-interview-ai-sessions-dev
FILES_TABLE=voice-interview-ai-files-dev

# Code Execution Settings
EXECUTION_TIMEOUT=30000
MAX_MEMORY=128m
USE_DOCKER_EXECUTION=true
USE_LOCAL_EXECUTION=false

# Docker Images
PYTHON_EXECUTOR_IMAGE=interview-executor:python
JAVASCRIPT_EXECUTOR_IMAGE=interview-executor:node

# Development Settings
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# API Keys (for Phase 2 - Voice Processing)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# WebSocket Settings (for Phase 2)
WEBSOCKET_ENDPOINT=wss://your-websocket-api-id.execute-api.us-east-1.amazonaws.com/dev

# Note: AWS credentials are automatically loaded from your AWS CLI profile
# Run 'aws configure list' to verify your credentials are set up correctly
